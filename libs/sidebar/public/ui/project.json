{
  "projectType": "library",
  "root": "libs/sidebar/public/ui",
  "sourceRoot": "libs/sidebar/public/ui/src",
  "prefix": "architect-poc",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/sidebar/public/ui"],
      "options": {
        "jestConfig": "libs/sidebar/public/ui/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/sidebar/public/ui/src/**/*.ts",
          "libs/sidebar/public/ui/src/**/*.html"
        ]
      }
    }
  },
  "tags": ["domain:public", "type:ui"]
}
