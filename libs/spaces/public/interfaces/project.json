      "options": {
        "lintFilePatterns": [
          "libs/spaces/public/interfaces/src/**/*.ts",
          "libs/spaces/public/interfaces/src/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:spaces-shared", "type:interfaces"]
