{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist/out-tsc",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2017",
    "module": "es2020",
    "lib": ["es2020", "dom"],
    "paths": {
      "@architect-poc/dashboards/data": ["libs/dashboards/data/src/index.ts"],
      "@architect-poc/dashboards/domain": [
        "libs/dashboards/domain/src/index.ts"
      ],
      "@architect-poc/dashboards/public/interfaces": [
        "libs/dashboards/public/interfaces/src/index.ts"
      ],
      "@architect-poc/dashboards/public/ui": [
        "libs/dashboards/public/ui/src/index.ts"
      ],
      "@architect-poc/dashboards/public/ui/feature-dashboard-list": [
        "libs/dashboards/public/ui/feature-dashboard-list/src/index.ts"
      ],
      "@architect-poc/dashboards/public/ui/feature-dashboard-view": [
        "libs/dashboards/public/ui/feature-dashboard-view/src/index.ts"
      ],
      "@architect-poc/dashboards/public/ui/shared": [
        "libs/dashboards/public/ui/shared/src/index.ts"
      ],
      "@architect-poc/dashboards/use-cases": [
        "libs/dashboards/use-cases/src/index.ts"
      ],
      "@architect-poc/design-system/public/ui": [
        "libs/design-system/public/ui/src/index.ts"
      ],
      "@architect-poc/settings/data": ["libs/settings/data/src/index.ts"],
      "@architect-poc/settings/domain": ["libs/settings/domain/src/index.ts"],
      "@architect-poc/settings/public/interfaces": [
        "libs/settings/public/interfaces/src/index.ts"
      ],
      "@architect-poc/settings/public/ui": [
        "libs/settings/public/ui/src/index.ts"
      ],
      "@architect-poc/settings/use-cases": [
        "libs/settings/use-cases/src/index.ts"
      ],
      "@architect-poc/sidebar/data": ["libs/sidebar/data/src/index.ts"],
      "@architect-poc/sidebar/domain": ["libs/sidebar/domain/src/index.ts"],
      "@architect-poc/sidebar/public/ui": [
        "libs/sidebar/public/ui/src/index.ts"
      ],
      "@architect-poc/sidebar/use-cases": [
        "libs/sidebar/use-cases/src/index.ts"
      ],
      "@architect-poc/spaces/data": ["libs/spaces/data/src/index.ts"],
      "@architect-poc/spaces/domain": ["libs/spaces/domain/src/index.ts"],
      "@architect-poc/spaces/public/ui": ["libs/spaces/public/ui/src/index.ts"],
      "@architect-poc/spaces/use-cases": ["libs/spaces/use-cases/src/index.ts"]
    },
    "rootDir": "."
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
